{
    "builds": [{
        "source": "hazeeeee/wsgi.py",
        "use": "@vercel/python",
        "config": {
            "maxLambdaSize": "15mb",
            "runtime": "python3.9"
        }
    }],
    "routes" : [
        {
            "source": "/(.*)",
            "destination": "hazeeeee/wsgi.py"
        }
    ]
}